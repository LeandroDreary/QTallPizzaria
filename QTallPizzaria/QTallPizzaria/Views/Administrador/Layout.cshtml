<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.css")"
          rel="stylesheet" />
    <link rel="icon" src="~/img/icon.png" />
</head>
<body>
    <ul class="nav bg-dark justify-content-center">
        <li class="nav-item">
            <a class="navbar-brand" href="#">
                <img src="~/img/icon.png" width="100" height="100" class="d-inline-block align-top" alt="">
            </a>
        </li>
    </ul>
        @RenderBody()
    <script src="@Url.Content("~/Scripts/jquery-3.3.1.min.js")">
    </script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")">
    </script>
    @RenderSection("script", required: false)
</body>
</html>